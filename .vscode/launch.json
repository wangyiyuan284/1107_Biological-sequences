        {
                "name": "Python", // The py file you want to debug should be opened and selected
                "type": "python",
                "request": "launch",
                "stopOnEntry": false, //Whether to enter the program Stop, false is not defined
                // "python": "C:\\Python36\\python.exe", 
                "python": "E:\\Python\\python.exe", 
                // Your own Python environment interpreter address, very important, if you have multiple Python interpreter needs to be set up
                "program": "${file}",
                "cwd": "${workspaceRoot}",
                "console": "integratedTerminal",
                "justMyCode": true, //Only debug your own Code
                "args": [
                    "input_data/user_specified.txt",
                    "output_data",
                    
                    ], //The parameter you need to enter when you want debug
                // "args": ["examples\\benchmarks\\LightGBM\\workflow_config_lightgbm_Alpha158_l1.yaml"], //需要输入的参数
                "env": {},
                "envFile": "${workspaceRoot}/.env",
                "redirectOutput": true
        }
    ]
}
